{
  "name": "pitomnik-myta",
  "version": "1.0.0",
  "description": "Бизнес-платформа для питомниководства с каталогом растений",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "parse-catalog": "node scripts/parse-catalog.js",
    "import-catalog": "node scripts/import-to-db.js"
  },
  "keywords": ["plants", "nursery", "catalog", "ecommerce"],
  "author": "Питомник МЯТА - Ирина Леонидовна",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "xlsx": "^0.18.5",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  }
}
